! ~/.Xresources
Xcursor.theme:  Adwaita

! fonts
i3.font:        pango:FuraCode Nerd Font 8
st.font:        FuraCode Nerd Font:pixelsize=12
dropdown.font:  FuraCode Nerd Font:pixelsize=12

#define default_bg   #2d2d2d
#define bright_bg    #393939
#define select_bg    #515151
#define line_hl      #747369
#define dark_fg      #a09f93
#define default_fg   #d3d0c8
#define light_fg     #e8e6df
#define light_bg     #f2f0ec
#define red          #f2777a
#define br_orange    #f99157
#define yellow       #ffcc66
#define green        #99cc99
#define cyan         #66cccc
#define blue         #6699cc
#define magenta      #cc99cc
#define orange       #d27b53

*base00: default_bg
*base01: bright_bg
*base02: select_bg
*base03: line_hl
*base04: dark_fg
*base05: default_fg
*base06: light_fg
*base07: light_bg

*foreground:   default_fg
*background:   default_bg
*cursorColor:  default_fg

! black
*color0:       default_bg
*color8:       bright_bg

! red
*color1:       red
*color9:       red

! green
*color2:       green
*color10:      green

! yellow
*color3:       yellow
*color11:      yellow

! blue
*color4:       blue
*color12:      blue

! magenta
*color5:       magenta
*color13:      magenta

! cyan
*color6:       cyan
*color14:      cyan

! white
*color7:       light_fg
*color15:      light_bg

! qutebrowser
qutebrowser.color16:  select_bg
qutebrowser.color17:  line_hl
qutebrowser.color18:  dark_fg
qutebrowser.color19:  default_fg

